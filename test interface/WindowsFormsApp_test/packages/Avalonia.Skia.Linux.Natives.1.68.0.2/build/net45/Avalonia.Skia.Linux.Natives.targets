<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

    <ItemGroup Condition=" '$(OS)' != 'Unix' and Exists('$(MSBuildThisFileDirectory)..\..\..\..\SkiaSharp\1.68.0\runtimes\osx\native\libSkiaSharp.dylib')">
